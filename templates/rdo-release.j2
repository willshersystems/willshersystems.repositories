# {{ ansible_managed }}
{% set rdo_release = name | replace("rdo-", "") %}
[openstack-{{ rdo_release }}]
name=OpenStack {{ rdo_release|upper }} Repository
baseurl=http://repos.fedorapeople.org/repos/openstack/openstack-{{ rdo_release }}/el{{ ansible_distribution_major_version }}/
skip_if_unavailable=0
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-RDO-{{ rdo_release }}
